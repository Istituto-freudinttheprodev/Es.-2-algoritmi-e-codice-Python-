    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="dollari, sterline, eurototali, dollariconversione, sterlineconversione, euroindollari, euroinsterline" type="Real" array="False" size=""/>
            <output expression="&quot;Inserisci il valore numerico dei soldi in dollari&quot;" newline="True"/>
            <input variable="dollari"/>
            <output expression="&quot;Inserisci il valore numerico dei soldi in sterline&quot;" newline="True"/>
            <input variable="sterline"/>
            <assign variable="dollariconversione" expression="0.85"/>
            <assign variable="sterlineconversione" expression="1.15"/>
            <assign variable="euroindollari" expression="dollari*dollariconversione"/>
            <assign variable="euroinsterline" expression="sterline*sterlineconversione"/>
            <assign variable="eurototali" expression="euroindollari+euroinsterline"/>
            <output expression="&quot;Il valore dei soldi in dollari in euro &#232;:&quot;" newline="True"/>
            <output expression="euroindollari" newline="True"/>
            <output expression="&quot;Il valore dei soldi in sterline in euro &#232;:&quot;" newline="True"/>
            <output expression="euroinsterline" newline="True"/>
            <output expression="&quot;Il valore della somma dei soldi di entrambe le valute in euro &#232;:&quot;" newline="True"/>
            <output expression="eurototali" newline="True"/>
        </body>
    </function>
