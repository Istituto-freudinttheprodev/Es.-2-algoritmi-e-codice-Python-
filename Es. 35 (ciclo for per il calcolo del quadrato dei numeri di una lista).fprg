    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="quadrato" type="Real" array="False" size=""/>
            <declare name="numeri" type="Real" array="True" size="3"/>
            <output expression="&quot;Inserisci 3 numeri di cui desideri sapere il quadrato (un numero a messaggio)&quot;" newline="True"/>
            <for variable="i" start="0" end="2" direction="inc" step="1">
                <input variable="numeri[i]"/>
            </for>
            <output expression="&quot;I quadrati dei numeri inseriti sono: &quot;" newline="True"/>
            <for variable="i" start="0" end="2" direction="inc" step="1">
                <assign variable="quadrato" expression="numeri[i]^2"/>
                <output expression="quadrato" newline="True"/>
            </for>
        </body>
