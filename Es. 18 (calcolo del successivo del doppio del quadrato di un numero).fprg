    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numero, risultato, quadrato, doppiodelquadrato" type="Real" array="False" size=""/>
            <output expression="&quot;Inserisci il valore del numero di cui desideri fare il calcolo&quot;" newline="True"/>
            <input variable="numero"/>
            <assign variable="quadrato" expression="numero*numero"/>
            <assign variable="doppiodelquadrato" expression="2*quadrato"/>
            <assign variable="risultato" expression="doppiodelquadrato+1"/>
            <output expression="&quot;Il valore del risultato del calcolo &#232;:&quot;" newline="True"/>
            <output expression="risultato" newline="True"/>
        </body>
