    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="lettera" type="String" array="False" size=""/>
            <output expression="&quot;Inserisci la lettera di cui desideri sapere se &#232; vocale o meno (inserire solo una lettera alla volta)&quot;" newline="True"/>
            <input variable="lettera"/>
            <if expression="lettera == &quot;a&quot;">
                <then>
                    <output expression="&quot;La lettera inserita &#232; una vocale&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="lettera == &quot;e&quot;">
                        <then>
                            <output expression="&quot;La lettera inserita &#232; una vocale&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="lettera == &quot;i&quot;">
                                <then>
                                    <output expression="&quot;La lettera inserita &#232; una vocale&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="lettera == &quot;o&quot;">
                                        <then>
                                            <output expression="&quot;La lettera inserita &#232; una vocale&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="lettera == &quot;u&quot;">
                                                <then>
                                                    <output expression="&quot;La lettera inserita &#232; una vocale&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;La lettera inserita non &#232; una vocale, bens&#236; una consonante, un numero o un carattere speciale&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
